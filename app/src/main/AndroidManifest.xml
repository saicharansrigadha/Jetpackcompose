<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/jetpack"
        android:label="@string/app_name"
        android:roundIcon="@drawable/jetpack"
        android:supportsRtl="true"
        android:theme="@style/Theme.JetpackCompose">
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".State.StateLessExample"
            android:exported="true">
        </activity>
        <activity
            android:name=".State.StateFulExample"
            android:exported="true">
        </activity>
        <activity
            android:name=".LazyColumn.ExampleColumn"
            android:exported="true">
        </activity>
    </application>

</manifest>